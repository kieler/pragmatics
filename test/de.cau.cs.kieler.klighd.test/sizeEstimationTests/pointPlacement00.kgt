
knode {
	properties: klighd.testing.expected.^height = 106.0
	klighd.testing.expected.^width = 194.0
	kroundedRectangle (20, 20) {
		kpolyline {
			points: left 50%, top ; left 50%, top 100%
			styles: foreground = 255r
		}
		kpolyline {
			points: left 75%, top ; left 75%, top 100%
			styles: lineWidth = 2 foreground = 255g
		}
		kpolyline {
			points: left 25%, top ; left 25%, top 100%
			styles: foreground = 255b
		}
		kpolyline {
			points: left, top 50% ; left 100%, top 50%
			styles: foreground = 255r, 2a -> 255r
		}
		krectangle {
			styles: foreground = 255g, 255b
			pointData: referencePoint = left 50%, top 50% horizontalAlignment =
			center verticalAlignment = center horizontalMargin = 15
			ktext ("________________|________________") [klighd.testing.^height = 10.0
			klighd.testing.^width = 164.0]
		}
		krectangle {
			styles: foreground = 255r, 255g
			pointData: referencePoint = left 10%, top 50% verticalAlignment = center
			ktext ("|\n|\n|\n|\n|\n|\n|\n|\n|\n") [klighd.testing.^height = 106.0
			klighd.testing.^width = 4.0]
		}
	}
}