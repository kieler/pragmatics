# User configuration file for the KWebS server

#
# Services to be published
#

# Use this option to configure whether the JAXWS layout will be published HTTP based 
#de.cau.cs.kieler.kwebs.server.jaxws.publishHttp=true

# Use this option to configure whether the JAXWS layout will be published HTTPS based 
#de.cau.cs.kieler.kwebs.server.jaxws.publishHttps=false

# Use this option to configure whether the support server will be published
#de.cau.cs.kieler.kwebs.publishSupportServer=true

#
# Publishing related settings
#

# Use this option to configure the address, port and context under which the JAXWS and HTTP based layout will be published 
#de.cau.cs.kieler.kwebs.server.jaxws.httpAddress=http://0.0.0.0:9442/layout

# Use this option to configure the address under that the JAXWS based layout via HTTP is visible from outside 
#de.cau.cs.kieler.kwebs.server.jaxws.publicHttpAddress=http://0.0.0.0:9442/layout

# Use this option to configure the address, port and context under which the JAXWS and HTTPS based layout will be published 
#de.cau.cs.kieler.kwebs.server.jaxws.httpsAddress=https://0.0.0.0:9443/layout

# Use this option to configure the address under that the JAXWS based layout via HTTPS is visible from outside 
#de.cau.cs.kieler.kwebs.server.jaxws.publicHttpsAddress=http://0.0.0.0:9443/layout

# Use this option to configure the address and port under which the support server will be published
#de.cau.cs.kieler.kwebs.supportServerAddress=http://0.0.0.0:9444

# Use this option to configure the address and port under which the support server is visible from outside 
#de.cau.cs.kieler.kwebs.publicSupportServerAddress=http://0.0.0.0:9444

#
# HTTPS related settings
#

# Use this option to configure the path to your servers key store
#de.cau.cs.kieler.kwebs.httpsKeystore.jks.path=

# Use this option to configure the password for your servers key store
#de.cau.cs.kieler.kwebs.httpsKeystore.jks.pass=

#
# Layout related settings
#

# Use this option to configure the path to the graphviz dot executable 
#de.cau.cs.kieler.kwebs.graphviz.path=

# Use this option to configure the timeout for the graphviz process
#de.cau.cs.kieler.kwebs.graphviz.timeout=

# Use this option to configure the timeout for the ogdf process
#de.cau.cs.kieler.kwebs.ogdf.timeout=

#
# Provider to use for creating the servers used for publishing the JAXWS web service
#

#de.cau.cs.kieler.kwebs.server.provider=sun
#de.cau.cs.kieler.kwebs.server.provider=jetty

#
# Web frontend configuration
#

#de.cau.cs.kieler.kwebs.frontend.service.hoster=
#de.cau.cs.kieler.kwebs.frontend.service.hoster.logo=
#de.cau.cs.kieler.kwebs.frontend.https.expose.truststore=false
#de.cau.cs.kieler.kwebs.frontend.disable.caching=false

#de.cau.cs.kieler.kwebs.frontend.contact.information.general=<Institution>\n\
