<plugin>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http://cs.cau.de/Graphs"
            class="de.cau.cs.kieler.graphs.GraphsPackage"
            genModel="model/graphs.genmodel"/>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="de.cau.cs.kieler.graphs.preferences.PreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <wizard
            canFinishEarly="false"
            class="de.cau.cs.kieler.graphs.wizards.CreateRandomModelWizard"
            hasPages="true"
            icon="icons/full/obj16/Graph.gif"
            id="de.cau.cs.rtprak.graphs.wizards.randomGraph"
            name="Create Random Graph"
            project="false">
      </wizard>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.eclipse.ui.ide.workbench"
            name="Graphs">
         <property
               name="appName"
               value="Graphs">
         </property>
         <property
               name="windowImages"
               value="icons/product/graphs16.png,icons/product/graphs32.png,icons/product/graphs48.png,icons/product/graphs64.png,icons/product/graphs128.png">
         </property>
         <property
               name="preferenceCustomization"
               value="plugin_customization.ini">
         </property>
      </product>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="de.cau.cs.kieler.graphs.product.GraphsPerspective"
            icon="icons/product/graphs16.png"
            id="de.cau.cs.kieler.graphs.perspective"
            name="Graphs">
      </perspective>
   </extension>

</plugin>
