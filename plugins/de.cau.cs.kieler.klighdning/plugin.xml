<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="Browser"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="de.cau.cs.kieler.klighdning.BrowseServerApplication">
         </run>
      </application>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="de.cau.cs.kieler.klighdning.Browser"
            name="KlighD SVG Browser">
         <property
               name="appName"
               value="KlighD SVG Browser">
         </property>
      </product>
   </extension>
</plugin>
