<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="de.cau.cs.kieler.kiml.layoutInfo">
      <config
            activation="de.cau.cs.kieler.kiml.evol"
            activationAction="de.cau.cs.kieler.kiml.evol.ui.ShowEvolutionAction$Activator"
            activationText="Activate Evolutionary Layout"
            class="de.cau.cs.kieler.kiml.evol.EvolLayoutConfig"
            default="false">
      </config>
   </extension>
   <extension
         point="de.cau.cs.kieler.kiml.service.analysisProviders">
      <category
            description="Analysis providers that yield a value between 0.0 and 1.0"
            id="de.cau.cs.kieler.kiml.evol.metricCategory"
            name="Normalized Metric">
      </category>
      <analysis
            category="de.cau.cs.kieler.kiml.evol.metricCategory"
            class="de.cau.cs.kieler.kiml.evol.metrics.AreaMetric"
            description="The area extent of the graph."
            id="de.cau.cs.kieler.kiml.evol.areaMetric"
            name="Area">
         <dependency
               analysis="de.cau.cs.kieler.kiml.grana.area">
         </dependency>
         <dependency
               analysis="de.cau.cs.kieler.kiml.grana.nodeCount">
         </dependency>
         <dependency
               analysis="de.cau.cs.kieler.kiml.grana.edgeCount">
         </dependency>
      </analysis>
      <analysis
            category="de.cau.cs.kieler.kiml.evol.metricCategory"
            class="de.cau.cs.kieler.kiml.evol.metrics.EdgeBendsMetric"
            description="The number of edge bends in the graph."
            id="de.cau.cs.kieler.kiml.evol.bendsMetric"
            name="Edge Bends">
         <dependency
               analysis="de.cau.cs.kieler.kiml.grana.bendpointCount">
         </dependency>
         <dependency
               analysis="de.cau.cs.kieler.kiml.grana.edgeCount">
         </dependency>
      </analysis>
      <analysis
            category="de.cau.cs.kieler.kiml.evol.metricCategory"
            class="de.cau.cs.kieler.kiml.evol.metrics.AspectRatioMetric"
            description="The aspect ratio of the graph, that is its width divided by its height."
            id="de.cau.cs.kieler.kiml.evol.aspectRatioMetric"
            name="Aspect Ratio">
         <dependency
               analysis="de.cau.cs.kieler.kiml.grana.area">
         </dependency>
      </analysis>
      <analysis
            category="de.cau.cs.kieler.kiml.evol.metricCategory"
            class="de.cau.cs.kieler.kiml.evol.metrics.EdgeCrossingsMetric"
            description="The number of edge crossings in the graph."
            id="de.cau.cs.kieler.kiml.evol.edgeCrossingsMetric"
            name="Edge Crossings">
         <dependency
               analysis="de.cau.cs.kieler.kiml.grana.bendpointCount">
         </dependency>
         <dependency
               analysis="de.cau.cs.kieler.kiml.grana.edgeCount">
         </dependency>
         <dependency
               analysis="de.cau.cs.kieler.kiml.grana.edgeCrossings">
         </dependency>
      </analysis>
      <analysis
            category="de.cau.cs.kieler.kiml.evol.metricCategory"
            class="de.cau.cs.kieler.kiml.evol.metrics.EdgeUniformityMetric"
            description="The edge length uniformity of the graph."
            id="de.cau.cs.kieler.kiml.evol.edgeUniformityMetric"
            name="Edge Uniformity">
         <dependency
               analysis="de.cau.cs.kieler.kiml.grana.edgeCount">
         </dependency>
      </analysis>
      <analysis
            category="de.cau.cs.kieler.kiml.evol.metricCategory"
            class="de.cau.cs.kieler.kiml.evol.metrics.UpwardnessMetric$UpMetric"
            description="The fraction of edges that point up."
            id="de.cau.cs.kieler.kiml.evol.upwardnessMetric"
            name="Upwardness">
         <dependency
               analysis="de.cau.cs.kieler.kiml.grana.edgeCount">
         </dependency>
         <dependency
               analysis="de.cau.cs.kieler.kiml.grana.edgeDirections">
         </dependency>
      </analysis>
      <analysis
            category="de.cau.cs.kieler.kiml.evol.metricCategory"
            class="de.cau.cs.kieler.kiml.evol.metrics.UpwardnessMetric$DownMetric"
            description="The fraction of edges that point down."
            id="de.cau.cs.kieler.kiml.evol.downwardnessMetric"
            name="Downwardness">
         <dependency
               analysis="de.cau.cs.kieler.kiml.grana.edgeCount">
         </dependency>
         <dependency
               analysis="de.cau.cs.kieler.kiml.grana.edgeDirections">
         </dependency>
      </analysis>
      <analysis
            category="de.cau.cs.kieler.kiml.evol.metricCategory"
            class="de.cau.cs.kieler.kiml.evol.metrics.UpwardnessMetric$LeftMetric"
            description="The fraction of edges that point left."
            id="de.cau.cs.kieler.kiml.evol.leftwardnessMetric"
            name="Leftwardness">
         <dependency
               analysis="de.cau.cs.kieler.kiml.grana.edgeCount">
         </dependency>
         <dependency
               analysis="de.cau.cs.kieler.kiml.grana.edgeDirections">
         </dependency>
      </analysis>
      <analysis
            category="de.cau.cs.kieler.kiml.evol.metricCategory"
            class="de.cau.cs.kieler.kiml.evol.metrics.UpwardnessMetric$RightMetric"
            description="The fraction of edges that point right."
            id="de.cau.cs.kieler.kiml.evol.rightwardnessMetric"
            name="Rightwardness">
         <dependency
               analysis="de.cau.cs.kieler.kiml.grana.edgeCount">
         </dependency>
         <dependency
               analysis="de.cau.cs.kieler.kiml.grana.edgeDirections">
         </dependency>
      </analysis>
      <analysis
            category="de.cau.cs.kieler.kiml.evol.metricCategory"
            class="de.cau.cs.kieler.kiml.evol.metrics.NodeSeparationMetric"
            description="Check whether the nodes have a minimal distance from each other."
            id="de.cau.cs.kieler.kiml.evol.nodeSeparationMetric"
            name="Node Separation">
      </analysis>
      <analysis
            category="de.cau.cs.kieler.kiml.evol.metricCategory"
            class="de.cau.cs.kieler.kiml.evol.metrics.EdgeOverlapsMetric"
            description="The number of node-edge overlaps in the graph."
            id="de.cau.cs.kieler.kiml.evol.edgeOverlapMetric"
            name="Edge Overlaps">
         <dependency
               analysis="de.cau.cs.kieler.kiml.grana.edgeCount">
         </dependency>
         <dependency
               analysis="de.cau.cs.kieler.kiml.grana.nodeEdgeOverlaps">
         </dependency>
      </analysis>
   </extension>

</plugin>
