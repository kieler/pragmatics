<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="de.cau.cs.kieler.kiml.layoutProviders">
      <layoutOption
            appliesTo="nodes"
            default="null"
            id="klighd.tooltip"
            name="Tooltip"
            type="string">
      </layoutOption>
   </extension>
   <extension
         point="de.cau.cs.kieler.klighd.extensions">
      <exporter
            class="de.cau.cs.kieler.klighd.piccolo.export.BitmapExporter"
            extension="png"
            id="de.cau.cs.kieler.klighd.piccolo.exporter.png"
            subFormat="png">
      </exporter>
      <exporter
            class="de.cau.cs.kieler.klighd.piccolo.export.BitmapExporter"
            extension="jpeg"
            id="de.cau.cs.kieler.klighd.piccolo.exporter.jpeg"
            subFormat="jpeg">
      </exporter>
      <exporter
            class="de.cau.cs.kieler.klighd.piccolo.export.BitmapExporter"
            extension="bmp"
            id="de.cau.cs.kieler.klighd.piccolo.exporter.bmp"
            subFormat="bmp">
      </exporter>
   </extension>

</plugin>
