<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="graphFormats" name="Graph Formats" schema="schema/graphFormats.exsd"/>

   <extension
         point="de.cau.cs.kieler.kiml.formats.graphFormats">
      <handler
            class="de.cau.cs.kieler.kiml.formats.KGraphHandler"
            description="Internally used graph format of the KIELER project. See http://rtsys.informatik.uni-kiel.de/confluence/x/U4EN"
            extensions="kgraph,kgx,xmi"
            id="de.cau.cs.kieler.kgraph"
            name="KGraph">
      </handler>
      <handler
            class="de.cau.cs.kieler.kiml.formats.matrix.MatrixFormatHandler"
            description="The Matrix Market format. See http://math.nist.gov/MatrixMarket/"
            extensions="mtx,matrix"
            id="gov.nist.math.matrix"
            name="Matrix">
      </handler>
   </extension>

</plugin>
