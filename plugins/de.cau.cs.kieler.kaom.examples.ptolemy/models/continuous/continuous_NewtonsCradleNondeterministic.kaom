      <annotations xsi:type="ann:TypedStringAnnotation" name="text" value="This variant relies on the tolerance of this model for&#xA;nondeterministic state machines in the modal model." type="ptolemy.kernel.util.StringAttribute"/>
      <annotations xsi:type="ann:TypedStringAnnotation" name="_location" value="[215.0, 360.0]" type="ptolemy.kernel.util.Location"/>
      <annotations xsi:type="ann:ReferenceAnnotation" name="attachedTo" object="//@childEntities.0/@childEntities.2"/>
    </annotations>
    <annotations xsi:type="ann:TypedStringAnnotation" name="annotation" type="ptolemy.kernel.util.Attribute">
      <annotations xsi:type="ann:TypedStringAnnotation" name="_hideName" type="ptolemy.kernel.util.SingletonAttribute"/>
      <annotations xsi:type="ann:TypedStringAnnotation" name="_iconDescription" type="ptolemy.kernel.util.SingletonConfigurableAttribute"/>
      <annotations xsi:type="ann:TypedStringAnnotation" name="_location" value="[205.0, 15.0]" type="ptolemy.kernel.util.Location"/>
      <annotations xsi:type="ann:TypedStringAnnotation" name="_controllerFactory" type="ptolemy.vergil.basic.NodeControllerFactory"/>
      <annotations xsi:type="ann:TypedStringAnnotation" name="_editorFactory" type="ptolemy.vergil.toolbox.AnnotationEditorFactory"/>
      <annotations xsi:type="ann:TypedStringAnnotation" name="text" value="This is a modified version of the Newton's cradle demo,&#xA;which needs to handle a nondeterministic state machine &#xA;to get the correct model's behavior.&#xA;&#xA;The key difference between this model and the normal &#xA;Newton's cradle demo is the initial states of the balls. &#xA;In this demo, all balls reside at their equilibrium points &#xA;initially. Ball #1 and ball #2 have non-zero initial &#xA;velocities, 0.3 and 0.2. Ball #3 has its initial velocity &#xA;as 0.0.&#xA;&#xA;With these initial settings, the state machine becomes &#xA;nondeterministic because there are multiple transitions &#xA;enabled at the start of the simulation (at time 0.0). The &#xA;interesting thing about this demo is that the behaviors &#xA;of the balls do not change even with a nondeterministic &#xA;order of executions of the enabled transitions. Look &#xA;inside the modal model to see more detailed explanation.&#xA;" type="ptolemy.kernel.util.StringAttribute"/>
      <annotations xsi:type="ann:ReferenceAnnotation" name="attachedTo"/>
    </annotations>
