               <appinfo>
                  <meta.attribute kind="java" basedOn=":de.cau.cs.kieler.kiml.service.formats.ITransformationHandler"/>
               </appinfo>
            </annotation>
         </attribute>
         <attribute name="description" type="string">
            <annotation>
               <documentation>
                  Description of the graph format.
               </documentation>
               <appinfo>
                  <meta.attribute translatable="true"/>
               </appinfo>
            </annotation>
         </attribute>
      </complexType>
