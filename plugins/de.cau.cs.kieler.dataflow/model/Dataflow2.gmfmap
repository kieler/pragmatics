<?xml version="1.0" encoding="UTF-8"?>
<gmfmap:Mapping xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore"
    xmlns:gmfmap="http://www.eclipse.org/gmf/2008/mappings"
    xmlns:gmftool="http://www.eclipse.org/gmf/2005/ToolDefinition">
  <nodes>
    <containmentFeature
        href="Dataflow.ecore#//Box/boxes"/>
    <childrenFeature
        href="Dataflow.ecore#//Box/boxes"/>
    <ownedChild>
      <domainMetaElement
          href="Dataflow.ecore#//Box"/>
      <domainInitializer
          xsi:type="gmfmap:FeatureSeqInitializer">
        <initializers
            xsi:type="gmfmap:FeatureValueSpec">
          <feature
              xsi:type="ecore:EAttribute"
              href="Dataflow.ecore#//Box/name"/>
          <value
              body="'Box'"/>
        </initializers>
      </domainInitializer>
      <labelMappings
          xsi:type="gmfmap:FeatureLabelMapping"
          viewPattern=""
          editorPattern=""
          editPattern="">
        <diagramLabel
            href="Dataflow.gmfgraph#BoxName"/>
        <features
            href="Dataflow.ecore#//Box/name"/>
      </labelMappings>
      <tool
          xsi:type="gmftool:CreationTool"
          href="Dataflow.gmftool#//@palette/@tools.0/@tools.0"/>
      <diagramNode
          href="Dataflow.gmfgraph#Box"/>
      <children
          referencedChild="//@nodes.0/@ownedChild/@children.0/@ownedChild">
        <containmentFeature
            href="Dataflow.ecore#//Box/inputs"/>
        <ownedChild>
          <domainMetaElement
              href="Dataflow.ecore#//InputPort"/>
          <labelMappings
              xsi:type="gmfmap:FeatureLabelMapping">
            <diagramLabel
                href="Dataflow.gmfgraph#PortName"/>
            <features
                href="Dataflow.ecore#//Port/name"/>
          </labelMappings>
          <tool
              xsi:type="gmftool:CreationTool"
              href="Dataflow.gmftool#//@palette/@tools.0/@tools.2"/>
          <diagramNode
              href="Dataflow.gmfgraph#InputPort"/>
        </ownedChild>
      </children>
      <children
          referencedChild="//@nodes.0/@ownedChild/@children.1/@ownedChild">
        <containmentFeature
            href="Dataflow.ecore#//Box/outputs"/>
        <ownedChild>
          <domainMetaElement
              href="Dataflow.ecore#//OutputPort"/>
          <labelMappings
              xsi:type="gmfmap:FeatureLabelMapping">
            <diagramLabel
                href="Dataflow.gmfgraph#PortName"/>
            <features
                href="Dataflow.ecore#//Port/name"/>
          </labelMappings>
          <tool
              xsi:type="gmftool:CreationTool"
              href="Dataflow.gmftool#//@palette/@tools.0/@tools.2"/>
          <diagramNode
              href="Dataflow.gmfgraph#OutputPort"/>
        </ownedChild>
      </children>
      <children
          compartment="//@nodes.0/@ownedChild/@compartments.0"
          referencedChild="//@nodes.0/@ownedChild">
        <containmentFeature
            href="Dataflow.ecore#//Box/boxes"/>
        <childrenFeature
            href="Dataflow.ecore#//Box/boxes"/>
      </children>
      <compartments
          children="//@nodes.0/@ownedChild/@children.2">
        <compartment
            href="Dataflow.gmfgraph#BoxCompartment"/>
      </compartments>
    </ownedChild>
  </nodes>
  <links>
    <domainMetaElement
        href="Dataflow.ecore#//Connection"/>
    <containmentFeature
        href="Dataflow.ecore#//Box/connections"/>
    <tool
        xsi:type="gmftool:CreationTool"
        href="Dataflow.gmftool#//@palette/@tools.0/@tools.1"/>
    <diagramLink
        href="Dataflow.gmfgraph#Connection"/>
    <sourceMetaFeature
        xsi:type="ecore:EReference"
        href="Dataflow.ecore#//Connection/sourcePort"/>
    <linkMetaFeature
        xsi:type="ecore:EReference"
        href="Dataflow.ecore#//Connection/targetPort"/>
  </links>
  <diagram>
    <diagramCanvas
        href="Dataflow.gmfgraph#dataflow"/>
    <domainModel
        href="Dataflow.ecore#/"/>
    <domainMetaElement
        href="Dataflow.ecore#//DataflowModel"/>
    <palette
        href="Dataflow.gmftool#//@palette"/>
  </diagram>
</gmfmap:Mapping>
