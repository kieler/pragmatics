         </property>
         <property
               name="preferenceCustomization"
               value="plugin_customization.ini">
         </property>
         <property
               name="defaultPerspectiveId"
               value="de.cau.cs.kieler.ptolemy.rcp.perspective">
         </property>
         <property
               name="startupProgressRect"
               value="19,301,207,15">
         </property>
         <property
               name="cssTheme"
