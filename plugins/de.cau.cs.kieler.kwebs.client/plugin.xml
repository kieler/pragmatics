<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="configurations" name="Client Configurations" schema="schema/configurations.exsd"/>
   <extension-point id="protocols" name="Protocol Support" schema="schema/protocols.exsd"/>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="de.cau.cs.kieler.kiml.ui.preferences.layout"
            class="de.cau.cs.kieler.kwebs.client.ui.RemoteLayoutPreferencePage"
            id="de.cau.cs.kieler.kwebs.client.ui.remotelayoutpreferencepage"
            name="Service Based Layout">
      </page>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="toolbar:org.eclipse.ui.trim.status">
         <toolbar
               id="de.cau.cs.kieler.kwebs.client.ui.kwebsstatusbar">
            <control
                  class="de.cau.cs.kieler.kwebs.client.ui.KWebSStatusBar"
                  id="de.cau.cs.kieler.kwebs.client.ui.kwebsstatusbar">
            </control>
         </toolbar>
      </menuContribution>
   </extension>
   <extension
         point="de.cau.cs.kieler.kiml.ui.layoutManagers">
      <engine
            class="de.cau.cs.kieler.kwebs.client.layout.RemoteGraphLayoutEngine"
            priority="5">
      </engine>
   </extension>
   <extension
         point="de.cau.cs.kieler.kwebs.client.protocols">
      <protocolSupport
            implementation="de.cau.cs.kieler.kwebs.client.jaxws.JaxWsClient"
            protocols="http;https">
      </protocolSupport>
   </extension>
   <extension
         point="de.cau.cs.kieler.kwebs.client.configurations">
      <defaultServerConfig
            address="http://layout.rtsys.informatik.uni-kiel.de:9442/layout"
            isStandard="true"
            name="KIELER Demo Layout Service (HTTP)">
      </defaultServerConfig>
   </extension>
   
</plugin>
