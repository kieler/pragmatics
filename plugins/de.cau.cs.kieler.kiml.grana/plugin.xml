<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="analysisProviders" name="AnalysisProviders" schema="schema/analysisProviders.exsd"/>
   <extension-point id="resultVisualizers" name="ResultVisualizers" schema="schema/resultVisualizers.exsd"/>

   <extension
         point="org.eclipse.ui.commands">
      <command
            categoryId="de.cau.cs.kieler.layout"
            defaultHandler="de.cau.cs.kieler.kiml.grana.ui.batch.BatchHandler"
            id="de.cau.cs.kieler.kiml.grana.command.setupBatch"
            name="Setup Batch">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.ui.popup.any?after=additions">
         <command
               commandId="de.cau.cs.kieler.kiml.grana.command.setupBatch"
               icon="icons/menu16/analyzediagram.gif"
               label="Setup Analysis Batch..."
               style="push"
               tooltip="Analyze multiple graphs in batch mode">
            <visibleWhen
                  checkEnabled="false">
               <reference
                     definitionId="de.cau.cs.kieler.kiml.grana.canSetupBatch">
               </reference>
            </visibleWhen>
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            allowMultiple="false"
            category="de.cau.cs.kieler.layout"
            class="de.cau.cs.kieler.kiml.grana.ui.views.AnalysisResultViewPart"
            icon="icons/menu16/analyzediagram.gif"
            id="de.cau.cs.kieler.kiml.grana.views.analysisResults"
            name="Graph Analysis"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.core.expressions.definitions">
      <definition
            id="de.cau.cs.kieler.kiml.grana.canSetupBatch">
         <with
               variable="selection">
            <iterate
                  ifEmpty="false"
                  operator="and">
               <or>
                 <instanceof
                     value="org.eclipse.core.resources.IFile">
                 </instanceof>
                 <instanceof
                     value="org.eclipse.core.resources.IFolder">
                 </instanceof>
                 <instanceof
                     value="org.eclipse.core.resources.IProject">
                 </instanceof>
               </or>
            </iterate>
         </with>
      </definition>
   </extension>
   <extension
         point="de.cau.cs.kieler.kiml.grana.resultVisualizers">
      <type
            name="text">
      </type>
      <type
            name="html">
      </type>
      <type
            name="csv">
      </type>
      <visualizer
            class="de.cau.cs.kieler.kiml.grana.ui.visualizers.ToStringVisualizer"
            priority="1"
            type="text">
      </visualizer>
      <visualizer
            class="de.cau.cs.kieler.kiml.grana.ui.visualizers.ArrayResultVisualizer"
            priority="10"
            type="text">
      </visualizer>
      <visualizer
            class="de.cau.cs.kieler.kiml.grana.ui.visualizers.AnalysisFailedHtmlVisualizer"
            priority="10"
            type="html">
      </visualizer>
      <visualizer
            class="de.cau.cs.kieler.kiml.grana.ui.visualizers.BasicHtmlVisualizer"
            priority="1"
            type="html">
      </visualizer>
      <visualizer
            class="de.cau.cs.kieler.kiml.grana.ui.visualizers.BasicCSVVisualizer"
            priority="1"
            type="csv">
      </visualizer>
      <visualizationMethod
            class="de.cau.cs.kieler.kiml.grana.ui.views.ViewVisualizationMethod"
            default="true"
            silent="true"
            type="html">
      </visualizationMethod>
   </extension>
   <extension
         point="de.cau.cs.kieler.kiml.grana.analysisProviders">
      <category
            description="This is the default category."
            id="de.cau.cs.kieler.kiml.grana.defaultCategory"
            name="Other">
      </category>
      <category
            description="Basic graph analyses."
            id="de.cau.cs.kieler.kiml.grana.basic"
            name="Basic Analyses">
      </category>
      <category
            description="Analyses that analyse the graph drawing."
            id="de.cau.cs.kieler.kiml.grana.drawing"
            name="Drawing Analyses">
      </category>
      <analysis
            category="de.cau.cs.kieler.kiml.grana.drawing"
            class="de.cau.cs.kieler.kiml.grana.analyses.AspectRatioAnalysis"
            description="The aspect ratio of the area a graph drawing occupies. (width / height)"
            id="de.cau.cs.kieler.kiml.grana.aspectRatio"
            name="Aspect Ratio">
         <dependency
               analysis="de.cau.cs.kieler.kiml.grana.area"
               weak="false">
         </dependency>
      </analysis>
      <analysis
            category="de.cau.cs.kieler.kiml.grana.basic"
            class="de.cau.cs.kieler.kiml.grana.analyses.BiconnectedComponentsAnalysis"
            description="The number of biconnected components, including hierarchies."
            id="de.cau.cs.kieler.kiml.grana.biconnectedComponents"
            name="Biconnected Components">
      </analysis>
      <analysis
            category="de.cau.cs.kieler.kiml.grana.basic"
            class="de.cau.cs.kieler.kiml.grana.analyses.CompoundEdgeAnalysis"
            description="Counts the number of hierarchy crossing edges and the maximal number of crossed hierarchy levels."
            id="de.cau.cs.kieler.kiml.grana.compoundEdge"
            name="Compound Edges">
         <component
               abbreviation="Count"
               name="Count">
         </component>
         <component
               abbreviation="MaxLevels"
               name="Max. Crossed Levels">
         </component>
         <component
               abbreviation="CompoundEdges"
               name="Incident to Compound Nodes">
         </component>
      </analysis>
      <analysis
            category="de.cau.cs.kieler.kiml.grana.basic"
            class="de.cau.cs.kieler.kiml.grana.analyses.CompoundNodeCountAnalysis"
            description="The number of compound nodes including hierarchies."
            id="de.cau.cs.kieler.kiml.grana.compoundNodeCount"
            name="Compound Node Count">
      </analysis>
      <analysis
            category="de.cau.cs.kieler.kiml.grana.basic"
            class="de.cau.cs.kieler.kiml.grana.analyses.CompoundNodeChildCountAnalysis"
            description="The minimum, average, and maximum number of nodes per hierarchy level."
            id="de.cau.cs.kieler.kiml.grana.compoundNodeChildCount"
            name="Compound Node Child Count">
         <component
               abbreviation="Min"
               name="Minimum">
         </component>
         <component
               abbreviation="Avg"
               name="Average">
         </component>
         <component
               abbreviation="Max"
               name="Maximum">
         </component>
      </analysis>
      <analysis
            category="de.cau.cs.kieler.kiml.grana.basic"
            class="de.cau.cs.kieler.kiml.grana.analyses.ConnectedComponentsAnalysis"
            description="The number of connected components, including hierarchies."
            id="de.cau.cs.kieler.kiml.grana.connectedComponents"
            name="Connected Components">
      </analysis>
      <analysis
            category="de.cau.cs.kieler.kiml.grana.drawing"
            class="de.cau.cs.kieler.kiml.grana.analyses.CoverageAnalysis"
            description="Percentage of area covered with nodes and taken up by insets."
            id="de.cau.cs.kieler.kiml.grana.coverage"
            name="Coverage">
         <dependency
               analysis="de.cau.cs.kieler.kiml.grana.nodeSize"
               weak="false">
         </dependency>
         <dependency
               analysis="de.cau.cs.kieler.kiml.grana.area"
               weak="false">
         </dependency>
      </analysis>
      <analysis
            category="de.cau.cs.kieler.kiml.grana.basic"
            class="de.cau.cs.kieler.kiml.grana.analyses.DirectedCycleApproximationAnalysis"
            description="An approximate number of directed cycles determined by the back edges found in a minimizing heuristic."
            id="de.cau.cs.kieler.kiml.grana.directedCyclesApprox"
            name="Directed Cycles Approximation">
      </analysis>
      <analysis
            category="de.cau.cs.kieler.kiml.grana.basic"
            class="de.cau.cs.kieler.kiml.grana.analyses.EdgeCountAnalysis"
            description="The number of edges including hierarchies."
            id="de.cau.cs.kieler.kiml.grana.edgeCount"
            name="Edge Count">
      </analysis>
      <analysis
            category="de.cau.cs.kieler.kiml.grana.basic"
            class="de.cau.cs.kieler.kiml.grana.analyses.EdgeLabelCountAnalysis"
            description="The number of edge labels."
            id="de.cau.cs.kieler.kiml.grana.edgeLabelCount"
            name="Edge Label Count">
      </analysis>
      <analysis
            category="de.cau.cs.kieler.kiml.grana.basic"
            class="de.cau.cs.kieler.kiml.grana.analyses.HyperNodeCountAnalysis"
            description="The number of hyper nodes including hierarchies."
            id="de.cau.cs.kieler.kiml.grana.hyperNodeCount"
            name="Hyper Node Count">
      </analysis>
      <analysis
            category="de.cau.cs.kieler.kiml.grana.basic"
            class="de.cau.cs.kieler.kiml.grana.analyses.MultiEdgeCountAnalysis"
            description="The number of multi edges including hierarchies."
            id="de.cau.cs.kieler.kiml.grana.multiEdgeCount"
            name="Multi Edge Count">
      </analysis>
      <analysis
            category="de.cau.cs.kieler.kiml.grana.basic"
            class="de.cau.cs.kieler.kiml.grana.analyses.NodeCountAnalysis"
            description="The number of nodes including hierarchies."
            id="de.cau.cs.kieler.kiml.grana.nodeCount"
            name="Node Count">
      </analysis>
      <analysis
            category="de.cau.cs.kieler.kiml.grana.basic"
            class="de.cau.cs.kieler.kiml.grana.analyses.NodeDegreeAnalysis"
            description="The minimum, average and maximum node degree of all nodes including hierarchies."
            id="de.cau.cs.kieler.kiml.grana.averageNodeDegree"
            name="Node Degree">
         <component
               abbreviation="Min"
               name="Minimum">
         </component>
         <component
               abbreviation="Avg"
               name="Average">
         </component>
         <component
               abbreviation="Max"
               name="Maximum">
         </component>
      </analysis>
      <analysis
            category="de.cau.cs.kieler.kiml.grana.drawing"
            class="de.cau.cs.kieler.kiml.grana.analyses.EdgeDirectionAnalysis"
            description="The number of edges going in each of the four directions."
            id="de.cau.cs.kieler.kiml.grana.edgeDirections"
            name="Edge Directions">
         <component
               name="Top">
         </component>
         <component
               name="Left">
         </component>
         <component
               name="Bottom">
         </component>
         <component
               name="Right">
         </component>
      </analysis>
      <analysis
            category="de.cau.cs.kieler.kiml.grana.drawing"
            class="de.cau.cs.kieler.kiml.grana.analyses.EdgeLengthAnalysis"
            description="The minimum, average, and maximum edge length and its variance."
            id="de.cau.cs.kieler.kiml.grana.edgeLength"
            name="Edge Length">
         <component
               abbreviation="Min"
               name="Minimum">
         </component>
         <component
               abbreviation="Avg"
               name="Average">
         </component>
         <component
               abbreviation="Max"
               name="Maximum">
         </component>
         <component
               name="Variance">
         </component>
      </analysis>
      <analysis
            category="de.cau.cs.kieler.kiml.grana.drawing"
            class="de.cau.cs.kieler.kiml.grana.analyses.HyperedgeCrossingsAnalysis"
            description="The number of hyperedge crossings."
            id="de.cau.cs.kieler.kiml.grana.hyperedgeCrossings"
            name="Hyperedge Crossings">
         <component
               abbreviation="Crossings"
               name="Crossings">
         </component>
         <component
               abbreviation="Hyperedges"
               name="Hyperedges">
         </component>
      </analysis>
      <analysis
            category="de.cau.cs.kieler.kiml.grana.drawing"
            class="de.cau.cs.kieler.kiml.grana.analyses.AreaAnalysis"
            description="The area a graph drawing occupies."
            id="de.cau.cs.kieler.kiml.grana.area"
            name="Area">
         <component
               name="Width">
         </component>
         <component
               name="Height">
         </component>
         <component
               name="Product">
         </component>
      </analysis>
      <analysis
            category="de.cau.cs.kieler.kiml.grana.drawing"
            class="de.cau.cs.kieler.kiml.grana.analyses.EdgeCrossingsAnalysis"
            description="The number of edge crossings, including hierarchies."
            id="de.cau.cs.kieler.kiml.grana.edgeCrossings"
            name="Number of Edge Crossings">
         <component
               abbreviation="Min"
               name="Minimum">
         </component>
         <component
               abbreviation="Avg"
               name="Average">
         </component>
         <component
               abbreviation="Max"
               name="Maximum">
         </component>
         <component
               abbreviation="Sum"
               name="Sum">
         </component>
         <component
               name="Onefold">
         </component>
      </analysis>
      <analysis
            category="de.cau.cs.kieler.kiml.grana.drawing"
            class="de.cau.cs.kieler.kiml.grana.analyses.NodeSizeAnalysis"
            description="The minimum, average and maximum node size."
            id="de.cau.cs.kieler.kiml.grana.nodeSize"
            name="Node Size">
         <component
               abbreviation="Min"
               name="Minimum">
         </component>
         <component
               abbreviation="Avg"
               name="Average">
         </component>
         <component
               abbreviation="Max"
               name="Maximum">
         </component>
         <component
               name="Nodes">
         </component>
      </analysis>
      <analysis
            category="de.cau.cs.kieler.kiml.grana.drawing"
            class="de.cau.cs.kieler.kiml.grana.analyses.BendsAnalysis"
            description="The number of bendpoints in the graph including hierarchies."
            id="de.cau.cs.kieler.kiml.grana.bendpointCount"
            name="Number of Bends">
         <component
               abbreviation="Min"
               name="Minimum">
         </component>
         <component
               abbreviation="Avg"
               name="Average">
         </component>
         <component
               abbreviation="Max"
               name="Maximum">
         </component>
         <component
               abbreviation="Sum"
               name="Sum">
         </component>
      </analysis>
      <analysis
            category="de.cau.cs.kieler.kiml.grana.drawing"
            class="de.cau.cs.kieler.kiml.grana.analyses.LayersAnalysis"
            description="Counts the number of horizontal and vertical layers in which the nodes are arranged."
            id="de.cau.cs.kieler.kiml.grana.layers"
            name="Number of Layers">
         <component
               abbreviation="Hor"
               name="Horizontal">
         </component>
         <component
               abbreviation="Ver"
               name="Vertical">
         </component>
         <component
               abbreviation="DN"
               name="Dummy Nodes">
         </component>
         <component
               abbreviation="MNPL"
               name="Max Nodes Per Layer">
         </component>
         <component
               abbreviation="MNPLD"
               name="Max Nodes Per Layer (Inc Dummies)">
         </component>
         <component
               abbreviation="ED"
               name="Edge Density (Adj. Layer)">
         </component>
      </analysis>
      <analysis
            category="de.cau.cs.kieler.kiml.grana.drawing"
            class="de.cau.cs.kieler.kiml.grana.analyses.NodeEdgeOverlapsAnalysis"
            description="The number of overlaps between nodes and edges on all hierarchy levels."
            id="de.cau.cs.kieler.kiml.grana.nodeEdgeOverlaps"
            name="Number of Node-Edge Overlaps">
      </analysis>
      <analysis
            category="de.cau.cs.kieler.kiml.grana.basic"
            class="de.cau.cs.kieler.kiml.grana.analyses.PortCountAnalysis"
            description="The number of ports including hierarchies."
            id="de.cau.cs.kieler.kiml.grana.portCount"
            name="Port Count">
      </analysis>
      <analysis
            category="de.cau.cs.kieler.kiml.grana.drawing"
            class="de.cau.cs.kieler.kiml.grana.analyses.PortSideAnalysis"
            description="The number of ports for each side a port can be attached to"
            id="de.cau.cs.kieler.kiml.grana.portSides"
            name="Port Sides">
         <component
               abbreviation="N"
               name="North">
         </component>
         <component
               abbreviation="E"
               name="East">
         </component>
         <component
               abbreviation="S"
               name="South">
         </component>
         <component
               abbreviation="W"
               name="West">
         </component>
      </analysis>
      <analysis
            category="de.cau.cs.kieler.kiml.grana.basic"
            class="de.cau.cs.kieler.kiml.grana.analyses.SelfLoopAnalysis"
            description="The number of self loops."
            id="de.cau.cs.kieler.kiml.grana.selfLoops"
            name="Self Loops">
      </analysis>
      <analysis
            category="de.cau.cs.kieler.kiml.grana.basic"
            class="de.cau.cs.kieler.kiml.grana.analyses.UndirectedCycleAnalysis"
            description="An approximate number of undirected cycles."
            id="de.cau.cs.kieler.kiml.grana.undirectedCycles"
            name="Undirected Cycles">
      </analysis>
      <analysis
            category="de.cau.cs.kieler.kiml.grana.basic"
            class="de.cau.cs.kieler.kiml.grana.analyses.LongestPathAnalysis"
            description="The number of edges on the longest path (for acyclic graphs)."
            id="de.cau.cs.kieler.kiml.grana.longestPath"
            name="Longest Path">
      </analysis>
      <analysis
            category="de.cau.cs.kieler.kiml.grana.drawing"
            class="de.cau.cs.kieler.kiml.grana.analyses.WhitespaceAnalysis"
            description="Determines the amount of whitespace in the diagram."
            id="de.cau.cs.kieler.kiml.grana.whitespace"
            name="Whitespace">
         <component
               abbreviation="TL"
               name="Top-Level">
         </component>
         <component
               abbreviation="H"
               name="Hierarchy">
         </component>
      </analysis>
      <analysis
            category="de.cau.cs.kieler.kiml.grana.drawing"
            class="de.cau.cs.kieler.kiml.grana.analyses.StressAnalysis"
            description="The stress of the graph."
            id="de.cau.cs.kieler.kiml.grana.stress"
            name="Stress">
         <component
               name="Center-To-Center">
         </component>
         <component
               name="Border-To-Border">
         </component>
         <component
               name="P-Stress">
         </component>
      </analysis>
      <analysis
            category="de.cau.cs.kieler.kiml.grana.drawing"
            class="de.cau.cs.kieler.kiml.grana.analyses.AxisParallelEdgesAnalysis"
            description="Determines the number of edges that are straight and axis aligned."
            id="de.cau.cs.kieler.kiml.grana.axisParallelEdges"
            name="Axis-Parallel Edges">
         <component
               name="Top">
         </component>
         <component
               name="Left">
         </component>
         <component
               name="Bottom">
         </component>
         <component
               name="Right">
         </component>
         <component
                name="Sum">
         </component>
      </analysis>
      <analysis
            category="de.cau.cs.kieler.kiml.grana.drawing"
            class="de.cau.cs.kieler.kiml.grana.analyses.AxisParallelEdgeSegmentsAnalysis"
            description="Determines the number of edge segments that are straight and axis aligned."
            id="de.cau.cs.kieler.kiml.grana.axisParallelEdgeSegments"
            name="Axis-Parallel Edge Segments">
         <component
               name="Top">
         </component>
         <component
               name="Left">
         </component>
         <component
               name="Bottom">
         </component>
         <component
               name="Right">
         </component>
         <component
                name="Sum">
         </component>
         <dependency
               analysis="de.cau.cs.kieler.kiml.grana.layers">
         </dependency>
      </analysis>
   </extension>

</plugin>
