   <!-- CATEGORY DEFINITIONS -->
   <category-def name="kieler-layout" label="KIELER Layout">
      <description>
         KIELER (Kiel Integrated Environment for Layout Eclipse RichClient) components for the automatic layout of diagrams.
      </description>
   </category-def>
   <category-def name="kieler-klighd" label="KIELER Lightweight Diagrams">
      <description>
         KIELER (Kiel Integrated Environment for Layout Eclipse RichClient) components for generation and displayal of transient, lightweight diagrams.
      </description>
   </category-def>
   <category-def name="kieler-editors" label="KIELER Editors">
      <description>
         KIELER (Kiel Integrated Environment for Layout Eclipse RichClient) components for editing diagrams and models.
         These editors are mostly meant for having something to test other KIELER components with.
      </description>
   </category-def>
   <category-def name="kieler-sources" label="KIELER Sources">
      <description>
         Source features for features of all other categories.
      </description>
   </category-def>


