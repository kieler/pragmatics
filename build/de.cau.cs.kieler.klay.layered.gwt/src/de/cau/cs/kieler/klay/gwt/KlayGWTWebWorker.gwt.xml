<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.5.1//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.5.1/distro-source/core/src/gwt-module.dtd">
<module rename-to='klay'>

  <!-- Inherits some modules, from Klay and GWT -->  
  <inherits name='de.cau.cs.kieler.klay.gwt.KlayGWTWebWorker'/>
  <inherits name='com.google.gwt.core.Core'/>
  <inherits name='com.google.gwt.json.JSON'/>
  <inherits name='de.cau.cs.kieler.Klay'/>

  <!-- Some properties must be set to generate just one permutation -->
  <set-configuration-property name="user.agent.runtimeWarning" value="false"/>	
  <set-configuration-property name="document.compatMode.severity" value="IGNORE" />
  <set-property name="user.agent" value="gecko1_8" />

  <!-- Define and specify the linker -->
  <define-linker name="workerlinker" class="de.cau.cs.kieler.klay.gwt.linker.WorkerLinker" />
  <add-linker name="workerlinker" />

  <!-- Specify the app entry point class.                         -->
  <entry-point class='de.cau.cs.kieler.klay.gwt.client.KlayGWTWebWorker'/>

  <!-- Specify the paths for translatable code                    -->
  <source path='client'/>
  <source path='shared'/>
</module>
